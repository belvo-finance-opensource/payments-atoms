{
  "name": "@belvo/payments-atoms",
  "description": "Library of native web components that aims to facilitate integrating with Belvo's Payments Widget.",
  "homepage": "https://belvo-finance-opensource.github.io/payments-atoms/docs/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belvo-finance-opensource/payments-atoms.git/"
  },
  "license": "MIT",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/belvo-payments-atoms.umd.js",
  "module": "./dist/belvo-payments-atoms.js",
  "exports": {
    ".": {
      "import": "./dist/belvo-payments-atoms.js",
      "require": "./dist/belvo-payments-atoms.umd.js"
    }
  },
  "types": "./dist/belvo-payments-atoms.d.ts",
  "scripts": {
    "build-only": "vite build",
    "build": "run-p type-check \"build-only {@}\" --",
    "watch": "vite build --watch",
    "test": "vitest",
    "test:ci": "vitest run --coverage --test-timeout=10000 --retry=2",
    "coverage": "vitest run --coverage",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "precommit": "lint-staged",
    "prepare": "is-ci || husky",
    "release": "release-it --verbose --ci",
    "typeconv": "npx typeconv -f ts -t oapi -o schemas/open-api 'src/types/**/pix.ts'"
  },
  "dependencies": {
    "@github/webauthn-json": "^2.1.1",
    "vue": "3.5.25"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@fingerprintjs/fingerprintjs-pro": "3.12.4",
    "@release-it/conventional-changelog": "9.0.4",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/addon-links": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/test": "8.6.14",
    "@storybook/vue3": "8.6.14",
    "@storybook/vue3-vite": "8.6.14",
    "@tsconfig/node18": "18.2.6",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.19.1",
    "@types/ua-parser-js": "0.7.39",
    "@types/uuid": "10.0.0",
    "@types/webappsec-credential-management": "0.6.9",
    "@vitejs/plugin-vue": "5.2.4",
    "@vitest/coverage-v8": "2.1.9",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.8.1",
    "ajv": "8.17.1",
    "auto-changelog": "2.5.0",
    "autoprefixer": "10.4.22",
    "commitlint-plugin-function-rules": "4.1.1",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "eslint": "9.39.1",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-storybook": "0.12.0",
    "eslint-plugin-unused-imports": "4.3.0",
    "eslint-plugin-vue": "9.33.0",
    "eslint-plugin-vue-scoped-css": "2.12.0",
    "globals": "15.15.0",
    "husky": "9.1.7",
    "is-ci": "3.0.1",
    "jsdom": "25.0.1",
    "lint-staged": "15.5.2",
    "npm-run-all2": "7.0.2",
    "postcss": "8.5.6",
    "prettier": "3.7.1",
    "prettier-plugin-organize-imports": "4.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "release-it": "17.11.0",
    "rfc4648": "1.5.4",
    "sass": "1.94.2",
    "sass-loader": "16.0.6",
    "storybook": "8.6.14",
    "typeconv": "2.3.1",
    "typescript": "5.9.3",
    "ua-parser-js": "1.0.41",
    "uuid": "11.1.0",
    "vite": "5.4.21",
    "vite-plugin-dts": "4.5.4",
    "vitest": "2.1.9",
    "vue-tsc": "2.2.12"
  }
}
